{% import "autoAPI/configFile" %}
{% func DB(configFile configFile.ConfigFile) %}
package infrastructure

import (
	"database/sql"
	_ "{%v configFile.Database.GetDBEngineModUrl() %}"
	"os"
)

var DB *sql.DB

func init() {
	var err error
	DB, err = sql.Open("{%v configFile.Database.GetDBEngine() %}", os.Getenv("DB_ADDRESS"))
	if err != nil {
		panic(err)
	}
}
{% endfunc %}
